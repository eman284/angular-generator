{
  "name": "th-ng-template-single-theme",
  "version": "1.0.0",
  "private": true,
  "description": "Thiqah Angular template for single theme handleing",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    <% if (includeSSR) { %>
      "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
      "serve:ssr": "node dist/server.js",
      "build:client-and-server-bundles": "npm run build-i18n && ng run <%= includeProjectName %>:server",
      "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    <% } %>
    "compodoc": "npx compodoc -p ./tsconfig.json -s",
    "build-dev": "ng build --prod --configuration dev",
    "build-test": "ng build --prod --configuration test",
    "build": "ng build --prod",
    "build:prod": "ng build --prod --configuration prod",
    "build:preprod": "ng build --prod --configuration preprod",
    "sass": "sass --watch src/assets/scss/style.scss:src/assets/css/ltr/style.css src/assets/scss/style-rtl.scss:src/assets/css/rtl/style.css --style compressed"
  },
  "keywords": [],
  "author": "Thiqah Frontend Team",
  "license": "Thiqah Frontend Team",
